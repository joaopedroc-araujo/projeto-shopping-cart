lor: white;\n  border: 0;\n  cursor: pointer;\n  font-size: 15px;\n  font-weight: 700;\n  width: 100%;\n  padding: 8px 10px;\n  border-radius: 5px;\n  transition: box-shadow 0.2s ease;\n}\n\n.product .product__add:hover {\n  background-color: rgb(25, 244, 255);\n  box-shadow: 0 0 0.2rem white,\n    0 0 0.2rem white,\n    0 0 1.2rem -5px rgb(0, 213, 226),\n    0 0 0.8rem -5px rgb(0, 213, 226),\n    0 0 1.6rem -5px rgb(0, 213, 226),\n    inset 0 0 1.3rem rgb(0, 138, 202);\n}\n\n.cart {\n  display: flex;\n  position: sticky;\n  top: 0;\n  width: 25%;\n  flex-direction: column;\n  height: 740px;\n  border-bottom-left-radius: 7px;\n  border: 1px solid rgb(240, 240, 240);\n  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.25);\n}\n\n.cart__products {\n  padding: 10px 30px 30px;\n  display: flex;\n  height: 100%;\n  flex-direction: column;\n  list-style: none;\n  overflow-y: scroll;\n}\n\n.cart__product {\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  padding: 10px 0;\n  margin: 10px 0;\n  height: 85px;\n  cursor: pointer;\n}\n\n.cart__product__info-container {\n  display: flex;\n  flex-direction: column;\n  align-items: flex-start;\n  justify-content: space-between;\n  width: 100%;\n  height: 100px;\n  padding: 0 20px;\n}\n\n.cart__product__info-container .product__price {\n  margin: 0;\n}\n\n.cart__product__remove {\n  background-color: transparent;\n  border: 0;\n  font-size: 20px;\n  color: rgb(168, 169, 171);\n  transition: color 0.2s ease;\n}\n\n.cart__product:hover .cart__product__remove {\n  color: rgb(219, 46, 46);\n}\n\n.cart__product__image-container {\n  width: 40%;\n  height: 100%;\n  overflow: hidden;\n  position: relative;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.cart__product__image-container img {\n  position: absolute;\n  max-width: 100%;\n  max-height: 100%;\n  transition: opacity 0.5s ease;\n}\n\n.cart__product__image-container img:last-child {\n  opacity: 0;\n}\n\n.cart__product__image-container:hover img:first-child {\n  opacity: 0;\n}\n\n.cart__product__image-container:hover img:last-child {\n  opacity: 1;\n}\n\n.cart-button {\n  width: 250px;\n  height: 30px;\n  margin: 0 auto;\n  border: 0;\n  color: white;\n  font-size: 16px;\n  font-weight: 700;\n  border-radius: 5px;\n  background-color: rgb(65, 25, 127);\n  cursor: pointer;\n  transition: box-shadow 0.2s ease;\n}\n\n.cart-button:hover {\n  background-color: rgb(109, 41, 212);\n  box-shadow: 0 0 0.2rem white,\n    0 0 0.2rem white,\n    0 0 1.4rem -5px rgb(131, 48, 255),\n    0 0 0.8rem -5px rgb(131, 48, 255),\n    0 0 2.4rem -5px rgb(131, 48, 255),\n    inset 0 0 2rem rgb(61, 11, 136);\n}\n\n.cep-input {\n  width: 250px;\n  font-size: 16px;\n  font-weight: 700;\n  margin: 20px auto 5px;\n}\n\n.cart__address {\n  height: 30px;\n  font-size: 16px;\n  font-weight: 700;\n  margin: 10px auto;\n}\n\n.hidden {\n  display: none;\n}\n\nfooter {\n  display: flex;\n  justify-content: center;\n  flex-direction: column;\n  align-items: center;\n  width: 100%;\n  height: 100%;\n  background-color: rgb(65, 25, 127);\n  color: white;\n  font-size: 10px;\n  font-weight: 700;\n  margin: 10px 0 10px 0;\n  padding: 10px 0;\n}\n\nfooter img {\n  margin: 5px;\n}\n\na {\n  color: white;\n  text-decoration: none;\n}\n\n::-webkit-scrollbar {\n  width: 8px;\n}\n\n::-webkit-scrollbar-thumb {\n  background: rgb(243, 243, 243);\n  box-shadow: inset 0 0 3px 1px rgb(155, 155, 155);\n  border-radius: 10px;\n}\n\n::-webkit-scrollbar-thumb:hover {\n  background: rgb(136, 244, 250);\n}\n\n::-webkit-scrollbar-track {\n  background: rgb(214, 214, 214);\n  border: 3px solid white;\n}\n\n@media screen and ( max-width : 1400px ) {\n\n  .cart {\n    height: 630px;\n  }\n}\n"},"lineCount":354}},"error":null,"hash":"b2e2882de1753b25ea1fa79ce2532ce7","cacheData":{"env":{}}}